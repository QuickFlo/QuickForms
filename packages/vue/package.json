    "vue3",
    "jsonschema",
    "forms",
    "validation",
    "veevalidate"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "codemirror": "^6.0.0",
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.8",
    "@quickflo/quickforms": "workspace:*",
    "vee-validate": "^4.12.0",
    "vue-codemirror": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0"
  }
